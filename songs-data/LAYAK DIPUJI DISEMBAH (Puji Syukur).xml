<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.1" modifiedIn="OpenLP 3.1.1" modifiedDate="2024-04-11T13:59:09">
  <properties>
    <titles>
      <title>LAYAK DIPUJI DISEMBAH</title>
    </titles>
    <authors>
      <author>Puji Syukur</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Alam semesta<br/>Melukiskan kebesaran Tuhan<br/>Bumi dan surga<br/>Menyanyikan kemuliaan namaMu</lines>
    </verse>
    <verse name="v1b">
      <lines>Layak dipuji disembah<br/>Kaulah yang bertahta<br/>Dulu skarang dan slamanya<br/>Raja atas sgala raja<br/>NamaMu besar termulia</lines>
    </verse>
    <verse name="v1c">
      <lines/>
    </verse>
    <verse name="v1d">
      <lines>Hosana, hosana</lines>
    </verse>
  </lyrics>
</song>
