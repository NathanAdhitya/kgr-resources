  <properties>
    <titles>
      <title>Soraklah Halleluya</title>
    </titles>
    <authors>
      <author>Alexander Groves</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Segala puji syukur hanya bagi-Mu Tuhan<br/>Sebab kau yang layak dipuja<br/>Kami mau bersorak tinggikan namaMu<br/>Halleluya</lines>
    </verse>
    <verse name="v1b">
      <lines>Soraklah halleluya soraklah halleluya<br/>Halleluya</lines>
    </verse>
    <verse name="v1c">
      <lines/>
    </verse>
  </lyrics>
