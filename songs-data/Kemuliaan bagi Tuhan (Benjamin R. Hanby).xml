  <properties>
    <titles>
      <title>Kemuliaan bagi Tuhan</title>
    </titles>
    <authors>
      <author>Benjamin R. Hanby</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>KEMULIAAN BAGI TUHAN<br/>DI TEMPAT MAHA TINGGI<br/>KAU RAJA DIAGUNGKAN DAN DIPUJI</lines>
    </verse>
    <verse name="v1b">
      <lines>DATANGLAH KERAJAAN-MU<br/>JADILAH KEHENDAK-MU<br/>S'LURUH BUMI PENUH KEMULIAAN-MU</lines>
    </verse>
    <verse name="v1c">
      <lines/>
    </verse>
  </lyrics>
