<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.1" modifiedIn="OpenLP 3.1.1" modifiedDate="2024-04-11T13:59:09">
  <properties>
    <titles>
      <title>S'gala puji syukur</title>
    </titles>
    <authors>
      <author>Ir. Niko Notorahardjo</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Segala puji syukur hanya bagi-Mu Tuhan<br/>Sebab Kau yang layak dipuja</lines>
    </verse>
    <verse name="v1b">
      <lines>Kami mau bersorak tinggikan Nama-Mu<br/>Haleluya</lines>
    </verse>
    <verse name="v1c">
      <lines>Soraklah Haleluya<br/>Soraklah Haleluya</lines>
    </verse>
    <verse name="v1d">
      <lines>Haleluya...</lines>
    </verse>
    <verse name="v1e">
      <lines/>
    </verse>
    <verse name="v1f">
      <lines>Shout for joy Halleluya</lines>
    </verse>
  </lyrics>
</song>
